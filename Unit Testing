import JUnit.framework.TestCase;

Public class CalendarTest extends TestCase {
	
	@Before1
	protected void setUp() throws Exception {
		MainActivity testActivity;
		Events	testEvent = new Events;
	}
	
	@Test1
	// adds as many events as possible
	public void overLoadEvents () {
		setUp();
		while(true) { // infinte loop
		testEvent = new Events;
		}
	}
	@Before2
	protected void setUp() throws Exception {
		MainActivity testActivity;
		Events testEvent = new Event;
		
		testEvent.setCategory("school");
	}
	@Test2
	// adds as many categories as possible
	public void overLoadCategories () {
		setUp();
		for(;;) {
		testEvent.setCategory();
		}
		
	}
	@Test
	// adds as conflicted events to see event conflict is working
	public void eventConflictCheck () {
		
	}
}
